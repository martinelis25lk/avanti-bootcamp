generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id            String   @id @default(uuid())
  email         String   @unique
  nome          String
  telefone      String?
  senha         String
  criado_em     DateTime
  atualizado_em DateTime @updatedAt
}
